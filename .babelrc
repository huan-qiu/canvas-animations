{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "ios": "8.4",
          "android": "4.4",
          "ie": "9"
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]
}
